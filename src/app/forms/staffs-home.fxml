<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="683.0" prefWidth="1096.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.forms.StaffsHome">
   <children>
      <MenuBar VBox.vgrow="NEVER">
         <menus>
            <Menu mnemonicParsing="false" text="Nhân viên">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#onGetInfo" text="Thông tin nhân viên" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" onAction="#onLogout" text="Đăng xuất" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
   </children>
</VBox>
